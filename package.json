{
  "name": "gatsby-starter-default-typescript",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Andy Kenward <hello@andykenward.com>",
  "dependencies": {
    "gatsby": "^2.9.4",
    "gatsby-image": "^2.2.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-sharp": "^2.2.0",
    "graphql": "^14.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "ts-optchain": "^0.1.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.0",
    "@graphql-codegen/typescript": "^1.3.0",
    "@graphql-codegen/typescript-operations": "^1.3.0",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run type-check",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andykenward/gatsby-starter-default-typescript"
  },
  "bugs": {
    "url": "https://github.com/andykenward/gatsby-starter-default-typescript/issues"
  }
}
